    "suiteName": "React Grid Demos",
    "output": "../../shippable/testresults/dx-react-grid-demos.xml"
  },
  "dependencies": {
    "@devexpress/dx-react-core": "1.0.0-alpha.3",
    "@devexpress/dx-react-grid": "1.0.0-alpha.3",
