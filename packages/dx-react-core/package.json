{
  "name": "@devexpress/dx-react-core",
  "version": "1.0.0-alpha.1",
  "description": "Core library for DevExtreme component suite for React",
  "author": {
    "name": "Developer Express Inc.",
    "url": "https://www.devexpress.com/"
  },
  "bugs": {
    "url": "https://github.com/DevExpress/devextreme-reactive/issues"
  },
  "homepage": "https://devexpress.github.io/devextreme-reactive/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevExpress/devextreme-reactive.git"
  },
  "keywords": [
    "js",
    "javascript",
    "widget",
    "component",
    "react"
  ],
  "license": "https://js.devexpress.com/Licensing/",
  "main": "dist/dx-react-core.cjs.js",
  "module": "dist/dx-react-core.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "lint": "eslint src/**/*",
    "lint:fix": "npm run -s lint -- --fix",
    "lint:ci": "npm run -s lint -- -o ../../shippable/testresults/dx-react-core-lint.xml -f junit"
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "jest-junit": {
    "suiteName": "React Core",
    "output": "../../shippable/testresults/dx-react-core.xml"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "core-js": "^2.4.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "jest-junit": "^1.5.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-license": "^0.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "@devexpress/dx-core": "^1.0.0-alpha.1",
    "babel-runtime": "^6.23.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4"
  }
}
