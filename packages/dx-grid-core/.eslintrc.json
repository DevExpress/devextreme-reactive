{
    "extends": "airbnb-base",
    "plugins": [
        "import",
        "jest",
        "filenames"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "jsx-a11y/href-no-hash": "off",
        "filenames/match-regex": [2, "^[a-z-\\.]+$"]
    },
    "env": {
      "jest/globals": true
    }
}
