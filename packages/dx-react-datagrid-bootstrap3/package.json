    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^1.0.0-alpha.1",
    "@devexpress/dx-react-datagrid": "^1.0.0-alpha.1",
