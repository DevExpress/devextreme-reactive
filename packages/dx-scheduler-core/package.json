    "test:coverage": "jest --coverage",
    "test:pacific": "TZ=US/Pacific jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "rrule": "2.6.4"
  },
  "peerDependencies": {
    "@devexpress/dx-core": "3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
