{
  "private": true,
  "name": "@devexpress/dx-react-grid-demos",
  "version": "2.7.1",
  "description": "DevExtreme React Components Demos",
  "author": {
    "name": "Developer Express Inc.",
    "url": "https://www.devexpress.com/"
  },
  "license": "SEE LICENSE IN README.md",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:generator": "node ../dx-demo-shell/generate-demos.js",
    "build": "yarn build:generator && webpack --env",
    "build:prod": "yarn build:generator && webpack --env.production",
    "build:watch:generator": "nodemon --exec \"yarn build:generator\" --watch \"src/demo-sources/**/*\" --ignore \"**/*.g.jsx\"  --ignore \"**/*.g.test.jsx\" --watch \"src/theme-sources/*/demo-source-data.json\" --watch \"generate-demos.js\" --ext js,jsx,jsxt,json,ts,tsx,tsxt",
    "build:watch": "concurrently \"yarn build:watch:generator\" \"yarn start-demo-server\"",
    "start-demo-server": "webpack-dev-server --env",
    "start-demo-server:prod": "webpack-dev-server --env.production",
    "escheck": "eslint \"src/**/*.{js,jsx}\" --ignore-pattern \"**/*.testcafe.js\" --ignore-pattern \"*.partial.jsx\"",
    "tscheck": "tslint \"src/**/*.{ts,tsx}\" --exclude \"src/**/*.g.test.tsx\"",
    "lint": "yarn escheck && yarn tscheck",
    "lint:fix": "yarn escheck -- --fix && yarn tscheck -- --fix"
  },
  "dependencies": {
    "@date-io/moment": "=1.3.13",
    "@devexpress/dx-chart-core": "2.7.1",
    "@devexpress/dx-demo-shell": "2.7.1",
    "@devexpress/dx-react-chart": "2.7.1",
    "@devexpress/dx-react-chart-bootstrap4": "2.7.1",
    "@devexpress/dx-react-chart-material-ui": "2.7.1",
    "@devexpress/dx-react-core": "2.7.1",
    "@devexpress/dx-react-grid": "2.7.1",
    "@devexpress/dx-react-grid-bootstrap3": "2.7.1",
    "@devexpress/dx-react-grid-bootstrap4": "2.7.1",
    "@devexpress/dx-react-grid-export": "2.7.1",
    "@devexpress/dx-react-grid-material-ui": "2.7.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@types/react": "16.9.35",
    "@types/react-dom": "^16.9.8",
    "clsx": "^1.1.1",
    "codemirror": "^5.49.2",
    "core-js": "^3.4.1",
    "d3-shape": "^1.3.7",
    "file-saver": "^2.0.2",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "=0.33.1",
    "react-dom": "^16.13.1",
    "react-frame-component": "^4.1.3",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.6.0",
    "redux": "^4.0.4",
    "seamless-immutable": "^7.1.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.8.3",
    "@devexpress/dx-testing": "2.7.1",
    "@types/jest": "^26.0.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0",
    "mustache": "^3.1.0",
    "node-static": "^0.7.11",
    "nodemon": "^1.19.4",
    "react-test-renderer": "^16.13.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.1.0",
