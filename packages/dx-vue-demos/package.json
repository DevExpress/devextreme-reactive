{
  "private": true,
  "name": "@devexpress/dx-vue-demos",
  "version": "1.3.0-beta.2",
  "description": "DevExtreme Vue Components Demos",
  "author": {
    "name": "Developer Express Inc.",
    "url": "https://www.devexpress.com/"
  },
  "license": "SEE LICENSE IN README.md",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:generator": "node ../dx-demo-shell/generate-demos.js",
    "build": "yarn run build:generator && webpack --env",
    "build:prod": "yarn run build:generator && webpack --env.production",
    "build:watch:generator": "nodemon --exec \"yarn run build:generator\" --watch \"src/demo-sources/**/*\" --ignore \"**/*.g.jsx\"  --ignore \"**/*.g.test.jsx\" --watch \"src/theme-sources/*/demo-source-data.json\" --watch \"generate-demos.js\" --ext js,jsx,jsxt,json,ts,tsx,tsxt",
    "build:watch": "concurrently \"yarn run build:watch:generator\" \"webpack-dev-server --env\"",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "yarn run lint -- --fix"
  },
  "dependencies": {
    "@devexpress/dx-demo-shell": "1.3.0-beta.2",
    "@devexpress/dx-vue-core": "1.3.0-beta.2",
    "@devexpress/dx-vue-grid": "1.3.0-beta.2",
    "@devexpress/dx-vue-grid-bootstrap4": "1.3.0-beta.2",
    "babel-polyfill": "^6.26.0",
    "prismjs": "^1.14.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "vue": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@devexpress/dx-testing": "1.3.0-beta.2",
    "@vue/test-utils": "1.0.0-beta.12",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-vue": "^4.5.0",
    "jest": "^23.1.0",
    "mustache": "^2.3.0",
    "nodemon": "^1.17.5",
    "style-loader": "^0.21.0",
    "vue-loader": "^15.2.2",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4",
    "write-file-webpack-plugin": "^4.3.2"
  }
}
