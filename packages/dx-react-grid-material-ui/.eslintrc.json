{
    "extends": "../../.eslintrc.react.json",
    "plugins": [
        "jest"
    ],
    "rules": {
        "jsx-a11y/no-static-element-interactions": "off",
        "filenames/match-regex": [2, "^[a-z-\\.]+$"],
        "no-restricted-imports": ["error", "@material-ui/core", "@material-ui/icons"]
    },
    "env": {
      "jest/globals": true
    }
}
