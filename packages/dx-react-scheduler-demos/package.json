    "react": "^16.8.3",
    "react-bootstrap": "=0.32.4",
    "react-dom": "^16.8.3",
    "react-frame-component": "^4.0.2",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "seamless-immutable": "^7.1.4",
    "whatwg-fetch": "^3.0.0"
