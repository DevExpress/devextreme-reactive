{
  "private": true,
  "name": "@devexpress/dx-vue-grid-demos",
  "version": "3.0.0-beta.2",
  "description": "DevExtreme Vue Components Demos",
  "author": {
    "name": "Developer Express Inc.",
    "url": "https://www.devexpress.com/"
  },
  "license": "SEE LICENSE IN README.md",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:generator": "node ../dx-demo-shell/generate-demos.js",
    "build": "yarn build:generator && webpack --env",
    "build:prod": "yarn build:generator && webpack --env.production",
    "build:watch:generator": "nodemon --exec \"yarn build:generator\" --watch \"src/demo-sources/**/*\" --ignore \"**/*.g.jsx\"  --ignore \"**/*.g.test.jsx\" --watch \"src/theme-sources/*/demo-source-data.json\" --watch \"generate-demos.js\" --ext js,jsx,jsxt,json,ts,tsx,tsxt",
    "build:watch": "concurrently \"yarn build:watch:generator\" \"yarn start-demo-server\"",
    "start-demo-server": "webpack-dev-server --env",
    "lint": "eslint \"src/**/*.{js,jsx}\" --ignore-pattern \"**/*.testcafe.js\"",
    "lint:fix": "yarn lint -- --fix"
  },
  "dependencies": {
    "@devexpress/dx-demo-shell": "3.0.0-beta.2",
    "@devexpress/dx-vue-core": "3.0.0-beta.2",
    "@devexpress/dx-vue-grid": "3.0.0-beta.2",
    "@devexpress/dx-vue-grid-bootstrap4": "3.0.0-beta.2",
    "babel-polyfill": "^6.26.0",
    "codemirror": "^5.49.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-bootstrap": "=0.33.1",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "vue": "^2.6.10",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@devexpress/dx-testing": "3.0.0-beta.2",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^27.4.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^6.4.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-vue": "^8.1.1",
    "jest": "^27.4.0",
    "mustache": "^4.2.0",
    "nodemon": "^2.0.15",
    "style-loader": "^3.3.1",
    "vue-loader": "^15.7.2",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
