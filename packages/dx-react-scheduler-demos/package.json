    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "@types/react": "16.9.35",
    "@types/react-dom": "^16.9.8",
    "clsx": "^1.0.4",
    "codemirror": "^5.49.2",
    "core-js": "^3.4.1",
    "d3-shape": "^1.3.7",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-bootstrap": "=0.33.1",
    "react-dom": "^16.14.0",
    "react-frame-component": "^4.1.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.4",
    "seamless-immutable": "^7.1.4",
    "whatwg-fetch": "^3.0.0"
  },
