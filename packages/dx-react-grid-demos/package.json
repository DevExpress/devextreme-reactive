{
  "private": true,
  "name": "@devexpress/dx-react-grid-demos",
  "version": "1.8.0",
  "description": "DevExtreme React Components Demos",
  "author": {
    "name": "Developer Express Inc.",
    "url": "https://www.devexpress.com/"
  },
  "license": "SEE LICENSE IN README.md",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:generator": "node ../dx-demo-shell/generate-demos.js",
    "build": "yarn run build:generator && webpack --env",
    "build:prod": "yarn run build:generator && webpack --env.production",
    "build:watch:generator": "nodemon --exec \"yarn run build:generator\" --watch \"src/demo-sources/**/*\" --ignore \"**/*.g.jsx\"  --ignore \"**/*.g.test.jsx\" --watch \"src/theme-sources/*/demo-source-data.json\" --watch \"generate-demos.js\" --ext js,jsx,jsxt,json,ts,tsx,tsxt",
    "build:watch": "concurrently \"yarn run build:watch:generator\" \"webpack-dev-server --env\"",
    "escheck": "eslint \"src/**/*.{js,jsx}\"",
    "tscheck": "tslint \"src/**/*.{ts,tsx}\" --exclude \"src/**/*.g.test.tsx\"",
    "lint": "yarn run escheck && yarn run tscheck",
    "lint:fix": "yarn run escheck -- --fix && yarn run tscheck -- --fix"
  },
  "dependencies": {
    "@devexpress/dx-demo-shell": "1.8.0",
    "@devexpress/dx-react-chart": "1.8.0",
    "@devexpress/dx-react-chart-bootstrap4": "1.8.0",
    "@devexpress/dx-react-chart-material-ui": "1.8.0",
    "@devexpress/dx-react-core": "1.8.0",
    "@devexpress/dx-react-grid": "1.8.0",
    "@devexpress/dx-react-grid-bootstrap3": "1.8.0",
    "@devexpress/dx-react-grid-bootstrap4": "1.8.0",
    "@devexpress/dx-react-grid-material-ui": "1.8.0",
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "core-js": "^2.5.7",
    "d3-shape": "^1.2.2",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-frame-component": "^4.0.2",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "seamless-immutable": "^7.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@devexpress/dx-testing": "1.8.0",
    "@types/jest": "^23.3.8",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.26.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "mustache": "^3.0.0",
