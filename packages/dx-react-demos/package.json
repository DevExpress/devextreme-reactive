    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "webpack --env",
    "build:prod": "webpack --env.production",
    "build:watch": "webpack-dev-server --env",
    "lint": "eslint src/**/*.{js,jsx}",
